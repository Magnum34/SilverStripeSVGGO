    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
      "expose": [
        "client/dist"
      ]
    },
  "autoload": {
    "psr-4": {
      "Magnum34\\SilverStripeSVGGO\\": "src/",
      "Magnum34\\SilverStripeSVGGO\\tests\\": "tests/"
    }
  }
}
